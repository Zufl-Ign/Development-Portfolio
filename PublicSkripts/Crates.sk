on right click:
    set {_h} to player's held item
    if name of {_h} is "&dLegendary Crate":
        cancel event
        if {_h} is a tripwire hook:
            if target block is yellow shulker box:
                remove 1 of {_h} from player
                set {_random} to random integer between 1 and 4
                if {_random} is 1:
                    send "&6[Mowing] &3You won SAD RANK!?" to player
                    give player 1 diamond named "&3Sad Rank"
                if {_random} is 2:
                    send "&6[Mowing] &3Pet Redeem Key!" to player
                    give player 1 Paper named "&6Pet Redeem Key!"
    if name of {_h} is "&fCommon Crate":
        cancel event
        if {_h} is a tripwire hook:
            if target block is white shulker box:
                remove 1 of {_h} from player
                set {_random} to random integer between 1 and 6
